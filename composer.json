{
  "name": "azgaou/mezzio-sentry",
  "type": "library",
  "description": "Fork of of the official mbiya135/mezzio-sentry Sentry ErrorHandler",
  "license": "MIT",
  "require": {
    "sentry/sdk": "^3.0",
    "mezzio/mezzio": "^3.3",
    "laminas/laminas-servicemanager": "3.7"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-strict-rules": "^1.4",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Mbiya135\\Mezzio\\Sentry\\": "src/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  }
}